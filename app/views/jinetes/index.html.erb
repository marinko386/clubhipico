<h1>Listing jinetes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jinetes.each do |jinete| %>
      <tr>
        <td><%= jinete.nombre %></td>
        <td><%= link_to 'Show', jinete %></td>
        <td><%= link_to 'Edit', edit_jinete_path(jinete) %></td>
        <td><%= link_to 'Destroy', jinete, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jinete', new_jinete_path %>
